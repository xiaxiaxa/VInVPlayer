<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xw.vplayer">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:name = "com.xw.vplayer.happyApplication"
        android:icon="@drawable/mgtv"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.xw.vplayer.activity.MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleInstance">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.xw.vplayer.activity.MyPlayerActivity"
            android:label="@string/app_name" />

        <activity
            android:name="com.xw.vplayer.activity.VlcPlayerActivity"
            android:label="@string/app_name" />

        <activity
            android:name="com.xw.vplayer.activity.IjkPlayerActivity"
            android:label="@string/app_name" />

    </application>

</manifest>
